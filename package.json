{
  "name": "@enonic/lib-admin-ui",
  "version": "6.0.0",
  "description": "Admin UI Library",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/enonic/lib-admin-ui"
  },
  "scripts": {
    "build": "pnpm build:dev",
    "build:prod": "pnpm --color /^build:prod:.*$/",
    "build:prod:js": "tsc -b",
    "build:prod:lib": "cross-env BUILD_TARGET=js vite build --mode production",
    "build:prod:css": "cross-env BUILD_TARGET=css vite build --mode production",
    "build:dev": "pnpm --color /^build:dev:.*$/",
    "build:dev:lib": "cross-env BUILD_TARGET=js vite build --mode development",
    "build:dev:css": "cross-env BUILD_TARGET=css vite build --mode development",
    "build:dev:js": "tsc -b",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "pnpm check:lint && pnpm check:types && pnpm test",
    "check:lint": "biome check .",
    "check:types": "tsc -b && tsc -p tsconfig.test.json",
    "fix": "biome check . --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky"
  },
  "dependencies": {
    "@enonic/ui": "~0.42.0",
    "dayjs": "^1.11.19",
    "dompurify": "~3.3.1",
    "fine-uploader": "^5.16.2",
    "jquery": "~3.7.1",
    "jquery-simulate": "^1.0.2",
    "jquery-ui": "^1.14.2",
    "lucide-react": "^0.540.0",
    "mousetrap": "^1.6.5",
    "nanoid": "~5.1.5",
    "nanostores": "~1.0.1",
    "preact": "^10.27.2",
    "q": "^1.5.1"
  },
  "peerDependencies": {
    "@radix-ui/react-slot": "^1.2.0",
    "focus-trap-react": "^11.0.0",
    "lucide-react": ">=0.500.0"
  },
  "devDependencies": {
    "@biomejs/biome": "~2.3.8",
    "husky": "^9.1.7",
    "@rollup/plugin-inject": "^5.0.5",
    "@storybook/addon-docs": "~10.1.6",
    "@storybook/addon-themes": "~10.1.6",
    "@storybook/preact-vite": "~10.1.6",
    "@tailwindcss/vite": "~4.1.18",
    "@types/jquery": "^3.5.33",
    "@types/jqueryui": "^1.12.24",
    "@types/mousetrap": "^1.6.15",
    "@types/q": "^1.5.8",
    "autoprefixer": "^10.4.24",
    "browserslist-config-enonic": "^1.0.8",
    "cross-env": "^10.1.0",
    "cssnano": "^7.1.2",
    "enonic-admin-artifacts": "^2.5.0",
    "jiti": "~2.6.1",
    "less": "^4.5.1",
    "postcss-normalize": "^13.0.1",
    "postcss-sort-media-queries": "^5.2.0",
    "storybook": "~10.1.10",
    "tailwindcss": "~4.1.18",
    "tw-animate-css": "~1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "~4.0.14"
  },
  "browserslist": [
    "extends browserslist-config-enonic"
  ],
  "packageManager": "pnpm@10.26.1",
  "engines": {
    "node": ">= 24.13.0",
    "pnpm": ">= 10.26.1"
  }
}
