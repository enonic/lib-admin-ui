{
  "name": "@enonic/lib-admin-ui",
  "version": "4.6.0",
  "description": "Admin UI Library",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/enonic/lib-admin-ui"
  },
  "main": "entry.js",
  "scripts": {
    "build:prod": "run-p build:prod:js build:lib",
    "build:prod:js": "tsc",
    "build:dev": "run-p build:dev:types build:dev:js build:lib",
    "build:dev:js": "swc src --out-dir build/tmp/src",
    "build:dev:types": "tsc --skipLibCheck --emitDeclarationOnly --declaration",
    "build:lib": "webpack --color",
    "typecheck": "tsc --pretty --skipLibCheck --noEmit",
    "lint": "eslint **/*.ts --quiet --cache",
    "fix": "eslint --fix **/*.ts --cache"
  },
  "dependencies": {
    "@types/dompurify": "^2.4.0",
    "@types/mousetrap": "^1.6.11",
    "@types/slickgrid": "^2.1.32",
    "dompurify": "^2.4.3",
    "fine-uploader": "^5.16.2",
    "jquery": "^3.6.3",
    "jquery-simulate": "^1.0.2",
    "jquery-ui": "^1.13.2",
    "mousetrap": "^1.6.5",
    "q": "^1.5.1",
    "slickgrid": "^3.0.2",
    "validator": "^13.9.0"
  },
  "peerDependencies": {
    "postcss": "^8.2.2"
  },
  "devDependencies": {
    "@enonic/eslint-config": "^1.0.0",
    "@swc/cli": "^0.1.61",
    "@swc/core": "^1.3.31",
    "@types/jquery": "^3.5.16",
    "@types/jqueryui": "^1.12.16",
    "@types/q": "^1.5.5",
    "@types/semver": "^7.3.13",
    "@types/validator": "^13.7.12",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "autoprefixer": "^10.4.13",
    "browserslist": "^4.21.5",
    "browserslist-config-enonic": "^1.0.7",
    "circular-dependency-plugin": "^5.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "cssnano": "^5.1.14",
    "enonic-admin-artifacts": "^1.8.3",
    "error-logger-webpack-plugin": "^1.1.1",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.0.2",
    "postcss-normalize": "^10.0.1",
    "postcss-sort-media-queries": "^4.3.0",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "browserslist": [
    "extends browserslist-config-enonic"
  ],
  "engines": {
    "node": ">= 18.12.1",
    "npm": ">= 8.19.2"
  }
}
