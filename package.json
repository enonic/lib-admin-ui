{
  "name": "@enonic/lib-admin-ui",
  "version": "6.0.0",
  "description": "Admin UI Library",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/enonic/lib-admin-ui"
  },
  "scripts": {
    "build": "pnpm build:dev",
    "build:prod": "pnpm --color /^build:prod:.*$/",
    "build:prod:js": "tsc --pretty",
    "build:prod:lib": "cross-env BUILD_TARGET=js vite build --mode production",
    "build:prod:css": "cross-env BUILD_TARGET=css vite build --mode production",
    "build:dev": "pnpm --color /^build:dev:.*$/",
    "build:dev:lib": "cross-env BUILD_TARGET=js vite build --mode development",
    "build:dev:css": "cross-env BUILD_TARGET=css vite build --mode development",
    "build:dev:js": "tsc --pretty --inlineSourceMap --inlineSources",
    "check": "eslint . --quiet --cache",
    "fix": "eslint . --fix --cache"
  },
  "dependencies": {
    "dompurify": "~3.3.0",
    "fine-uploader": "^5.16.2",
    "jquery": "^3.7.1",
    "jquery-simulate": "^1.0.2",
    "jquery-ui": "^1.14.1",
    "mousetrap": "^1.6.5",
    "q": "^1.5.1"
  },
  "devDependencies": {
    "@enonic/eslint-config": "^2.1.0",
    "@rollup/plugin-inject": "^5.0.5",
    "@types/jquery": "^3.5.33",
    "@types/jqueryui": "^1.12.24",
    "@types/mousetrap": "^1.6.15",
    "@types/postcss-normalize": "~9.0.4",
    "@types/q": "^1.5.8",
    "autoprefixer": "^10.4.21",
    "browserslist-config-enonic": "^1.0.8",
    "cross-env": "^10.1.0",
    "cssnano": "^7.1.2",
    "enonic-admin-artifacts": "^2.4.0",
    "eslint": "^9.39.1",
    "jiti": "~2.6.1",
    "less": "^4.4.2",
    "postcss-normalize": "^13.0.1",
    "postcss-sort-media-queries": "^5.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.2"
  },
  "browserslist": [
    "extends browserslist-config-enonic"
  ],
  "engines": {
    "node": ">= 22.19.0",
    "pnpm": ">= 10.17.1",
    "npm": ">= 10.9.3"
  }
}
